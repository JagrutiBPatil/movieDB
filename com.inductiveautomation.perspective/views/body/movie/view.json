      "system": {
        "onStartup": {
          "config": {
            "script": "\t# API URL and Headers\n\turl \u003d \"https://api.themoviedb.org/3/movie/popular?language\u003den-US\u0026page\u003d1\"\n\theaders \u003d {\n\t    \"accept\": \"application/json\",\n\t    \"Authorization\": \"Bearer eyJhbGciOiJIUzI1NiJ9.eyJhdWQiOiIyNTcyMTU2ZDQ3YjllM2U0M2RhMjA0MDRhMDQwYzNhOCIsIm5iZiI6MTczNjIyMzM0My44MjksInN1YiI6IjY3N2NhYTZmZDEwMmU3NzZmNTc0ODhiYiIsInNjb3BlcyI6WyJhcGlfcmVhZCJdLCJ2ZXJzaW9uIjoxfQ.nwAHnyYb8Ej1UCswTkltuNKZsCWWeknfHz_6Y0oi2es\"\n\t}\n\t\n\t# Create an HTTP client\n\thttp_client \u003d system.net.httpClient()\n\tresponse \u003d http_client.get(url, headers\u003dheaders)\n\t\n\t# Decode the JSON response\n\tresult \u003d system.util.jsonDecode(response.getText())\n\tmovies \u003d result.get(\"results\", [])\n\t\n\t# Get reference to the Flex Repeater\n\tflex_repeater \u003d self.getChild(\"FlexRepeater\")\n\t\n\t# Create instances list dynamically\n\tinstances \u003d []\n\tfor movie in movies:\n\t    instance \u003d {\n\t        \"viewPath\": \"New Folder/card3\",  # The path to your embedded view\n\t        \"params\": {\n\t            \"title\": movie.get(\"title\", \"Unknown Title\"),\n\t            \"imgPath\": \"https://image.tmdb.org/t/p/w500/\" + movie.get(\"poster_path\", \"\"),\n\t            \"rate\": movie.get(\"vote_average\", \"N/A\")\n\t        }\n\t    }\n\t    instances.append(instance)\n\t\n\t# Assign the instances to the Flex Repeater\n\tflex_repeater.props.instances \u003d instances\n\t\t\t"
          },
          "scope": "G",
          "type": "script"
        }
